        .hover\:bg-orange-600:hover { background-color: #ea580c; }
        .hover\:bg-gray-100:hover { background-color: #f3f4f6; }
        .hover\:bg-white:hover { background-color: white; }
        .hover\:text-primary:hover { color: #F58549; }
        .hover\:text-secondary:hover { color: #2F5047; }
        .hover\:shadow-lg:hover { box-shadow: 0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1); }
        .hover\:shadow-xl:hover { box-shadow: 0 20px 25px -5px rgb(0 0 0 / 0.1), 0 10px 10px -5px rgb(0 0 0 / 0.1); }
        .object-cover { object-fit: cover; }
        .z-50 { z-index: 50; }
        .flex-shrink-0 { flex-shrink: 0; }
        .bg-opacity-10 { background-color: rgba(255, 255, 255, 0.1); }
        .inline-block { display: inline-block; }
        
        @media (min-width: 640px) {
