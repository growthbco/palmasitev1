---
// Dunnellon Move-in/Move-out Cleaning service page
import Navigation from '../components/Navigation.astro';
import Footer from '../components/Footer.astro';
const title = "Dunnellon Move-in/Move-out Cleaning | Palma Cleaning - Marion County FL";
const description = "Professional move-in and move-out cleaning services for Dunnellon properties in Marion County, FL. Real estate ready cleaning, vacation rental turnover, and transition cleaning. Licensed & insured.";
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
    <meta name="description" content={description}>
    <meta name="keywords" content="Dunnellon move in cleaning, Dunnellon move out cleaning, Dunnellon real estate cleaning, Dunnellon vacation rental cleaning Marion County FL">
    <meta name="robots" content="index, follow">
    
    <link rel="canonical" href="https://palmacleaningco.com/dunnellon-move-in-out-cleaning">
    
    <!-- Favicon -->
    <link rel="icon" type="image/svg+xml" href="/favicon.svg">
    <link rel="stylesheet" href="/styles-clean.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Service",
        "name": "Dunnellon Move-in Move-out Cleaning",
        "description": "Professional move-in and move-out cleaning services for Dunnellon properties",
        "provider": {
            "@type": "LocalBusiness",
            "name": "Palma Cleaning",
            "telephone": "(352) 764-4747",
            "url": "https://palmacleaningco.com"
        },
        "areaServed": {
            "@type": "Place",
            "name": "Dunnellon, Marion County, FL"
        },
        "offers": {
            "@type": "Offer",
            "description": "Move-in cleaning, move-out cleaning, real estate ready cleaning, vacation rental turnover"
        }
    }
    </script>
</head>
<body>
    <Navigation />

    <section class="hero">
        <div class="hero-container">
            <div class="hero-content">
                <h1 class="hero-title">Dunnellon Move-in/Move-out Cleaning</h1>
                <p class="hero-subtitle">Complete Transition Cleaning for Dunnellon Properties</p>
                <p class="hero-description">Professional move-in and move-out cleaning services for Dunnellon's historic homes, riverfront properties, and vacation rentals. Make your transition smooth with thorough, real estate-ready cleaning that meets the standards of this charming river community.</p>
                <div class="hero-buttons">
                    <a href="/contact" class="btn btn-primary">Book Now</a>
                    <a href="tel:+13527644747" class="btn btn-secondary">Call (352) 764-4747</a>
                </div>
            </div>
            <div class="hero-image">
                <div class="hero-placeholder">
                    <i class="fas fa-box-open"></i>
                </div>
            </div>
        </div>
    </section>

    <section class="services">
        <div class="container">
            <h2 class="section-title">Dunnellon Move-in/Move-out Services</h2>
            <div style="max-width: 800px; margin: 0 auto 3rem;">
                <p style="font-size: 1.1rem; color: #666; text-align: center; line-height: 1.6;">Moving in Dunnellon requires special attention to the unique character of this historic river community. Whether you're moving into a charming downtown home, riverfront property, or rural estate, our comprehensive cleaning ensures a fresh, welcoming start or perfect farewell.</p>
            </div>
            
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2rem; margin-bottom: 3rem;">
                <div style="background: #f8fafa; padding: 2rem; border-radius: 15px;">
                    <h3 style="color: #2f5047; margin-bottom: 1rem;"><i class="fas fa-home" style="color: #ff8057; margin-right: 0.5rem;"></i>Move-in Cleaning</h3>
                    <ul style="color: #666; line-height: 1.8;">
                        <li><strong>Fresh Start:</strong> Complete sanitization before moving in</li>
                        <li><strong>Historic Homes:</strong> Specialized care for older properties</li>
                        <li><strong>River Properties:</strong> Humidity and moisture management</li>
                        <li><strong>Same-day Service:</strong> Available for urgent moves</li>
                    </ul>
                </div>
                
                <div style="background: #f8fafa; padding: 2rem; border-radius: 15px;">
                    <h3 style="color: #2f5047; margin-bottom: 1rem;"><i class="fas fa-key" style="color: #ff8057; margin-right: 0.5rem;"></i>Move-out Cleaning</h3>
                    <ul style="color: #666; line-height: 1.8;">
                        <li>Real estate ready presentation</li>
                        <li>Security deposit recovery focused</li>
                        <li>Vacation rental turnover</li>
                        <li>Community standard compliance</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

    <section class="about" style="background: #f8fafa;">
        <div class="container">
            <h2 class="section-title">Complete Dunnellon Move-in/Move-out Cleaning</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2rem;">
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-utensils" style="margin-right: 0.5rem;"></i>Kitchen Deep Clean</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Inside and outside all appliances</li>
                        <li>Cabinet interior and exterior cleaning</li>
                        <li>Countertop deep cleaning and sanitizing</li>
                        <li>Sink and faucet restoration</li>
                        <li>Backsplash and tile cleaning</li>
                        <li>Floor deep cleaning</li>
                        <li>Light fixture cleaning</li>
                    </ul>
                </div>
                
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-bath" style="margin-right: 0.5rem;"></i>Bathroom Restoration</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Complete sanitization and disinfection</li>
                        <li>Tile and grout deep cleaning</li>
                        <li>Shower and tub restoration</li>
                        <li>Toilet deep cleaning</li>
                        <li>Mirror and fixture polishing</li>
                        <li>Exhaust fan cleaning</li>
                        <li>Floor and baseboard cleaning</li>
                    </ul>
                </div>
                
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-bed" style="margin-right: 0.5rem;"></i>All Living Areas</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Thorough dusting of all surfaces</li>
                        <li>Deep carpet cleaning and vacuuming</li>
                        <li>Hard floor cleaning and restoration</li>
                        <li>Window and sill cleaning</li>
                        <li>Baseboard and trim detailing</li>
                        <li>Light switch and outlet cleaning</li>
                        <li>Ceiling fan and fixture cleaning</li>
                    </ul>
                </div>
                
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-star" style="margin-right: 0.5rem;"></i>Dunnellon Specialties</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Historic home preservation care</li>
                        <li>River humidity treatment</li>
                        <li>Vacation rental preparation</li>
                        <li>Real estate showing ready</li>
                        <li>Community standard compliance</li>
                        <li>Same-day turnaround available</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

    <section class="services">
        <div class="container">
            <h2 class="section-title">Why Choose Our Dunnellon Move-in/Move-out Cleaning</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); gap: 2rem; margin-bottom: 3rem;">
                <div style="text-align: center; padding: 2rem;">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem;">
                        <i class="fas fa-clock"></i>
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Fast Turnaround</h4>
                    <p style="color: #666; line-height: 1.6;">Quick, efficient service that meets your moving timeline, with same-day cleaning available for urgent transitions.</p>
                </div>
                
                <div style="text-align: center; padding: 2rem;">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem;">
                        <i class="fas fa-home"></i>
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Real Estate Ready</h4>
                    <p style="color: #666; line-height: 1.6;">Thorough cleaning that meets real estate standards and helps ensure security deposit returns for Dunnellon properties.</p>
                </div>
                
                <div style="text-align: center; padding: 2rem;">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem;">
                        <i class="fas fa-water"></i>
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">River Community Knowledge</h4>
                    <p style="color: #666; line-height: 1.6;">Understanding of Dunnellon's unique river environment challenges and specialized cleaning for historic properties.</p>
                </div>
                
                <div style="text-align: center; padding: 2rem;">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem;">
                        <i class="fas fa-shield-alt"></i>
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Satisfaction Guaranteed</h4>
                    <p style="color: #666; line-height: 1.6;">Complete satisfaction guarantee ensures your Dunnellon property transition goes smoothly with professional results.</p>
                </div>
            </div>
        </div>
    </section>

    <section class="contact" style="background: #f8fafa;">
        <div class="container">
            <h2 class="section-title">Ready for Dunnellon Move-in/Move-out Cleaning?</h2>
            <div style="text-align: center; max-width: 600px; margin: 0 auto;">
                <p style="font-size: 1.1rem; color: #666; margin-bottom: 2rem; line-height: 1.6;">Make your Dunnellon property transition smooth and stress-free with our professional move-in/move-out cleaning. Contact us for your free quote!</p>
                <div style="display: flex; gap: 1rem; justify-content: center; flex-wrap: wrap; margin-bottom: 2rem;">
                    <a href="/contact" class="btn btn-primary">Book Now</a>
                    <a href="tel:+13527644747" class="btn btn-secondary">Call (352) 764-4747</a>
                </div>
                <div style="background: white; padding: 1.5rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <p style="color: #2f5047; font-weight: 500; margin: 0;">
                        <i class="fas fa-check-circle" style="color: #ff8057; margin-right: 0.5rem;"></i>
                        Licensed & Insured • Fast Turnaround • Satisfaction Guaranteed
                    </p>
                </div>
            </div>
        </div>
    </section>

    <section class="services">
        <div class="container">
            <h2 class="section-title">Other Dunnellon Cleaning Services</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); gap: 1.5rem;">
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-home" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
