---
// Dunnellon Standard Cleaning service page
import Navigation from '../components/Navigation.astro';
import Footer from '../components/Footer.astro';
const title = "Dunnellon Standard House Cleaning | Palma Cleaning - Marion County FL";
const description = "Regular house cleaning services for Dunnellon community in Marion County, FL. Weekly, bi-weekly, and monthly cleaning plans for Dunnellon homes along the Rainbow River. Licensed & insured.";
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
    <meta name="description" content={description}>
    <meta name="keywords" content="Dunnellon house cleaning, Dunnellon standard cleaning, Dunnellon weekly cleaning, Dunnellon river community cleaning Marion County FL">
    <meta name="robots" content="index, follow">
    
    <link rel="canonical" href="https://palmacleaningco.com/dunnellon-standard-cleaning">
    
    <!-- Favicon -->
    <link rel="icon" type="image/svg+xml" href="/favicon.svg">
    <link rel="stylesheet" href="/styles-clean.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Service",
        "name": "Dunnellon Standard House Cleaning",
        "description": "Regular house cleaning services for Dunnellon community homes",
        "provider": {
            "@type": "LocalBusiness",
            "name": "Palma Cleaning",
            "telephone": "(352) 764-4747",
            "url": "https://palmacleaningco.com"
        },
        "areaServed": {
            "@type": "Place",
            "name": "Dunnellon, Marion County, FL"
        },
        "offers": {
            "@type": "Offer",
            "description": "Weekly, bi-weekly, and monthly house cleaning plans"
        }
    }
    </script>
</head>
<body>
    <Navigation />

    <section class="hero">
        <div class="hero-container">
            <div class="hero-content">
                <h1 class="hero-title">Dunnellon Standard House Cleaning</h1>
                <p class="hero-subtitle">Regular Cleaning for Dunnellon's Historic & Riverfront Homes</p>
                <p class="hero-description">Professional standard house cleaning specifically designed for Dunnellon residents. Keep your historic downtown home or riverfront property pristine with our reliable weekly, bi-weekly, or monthly cleaning service.</p>
                <div class="hero-buttons">
                    <a href="/contact" class="btn btn-primary">Book Now</a>
                    <a href="tel:+13527644747" class="btn btn-secondary">Call (352) 764-4747</a>
                </div>
            </div>
            <div class="hero-image">
                <div class="hero-placeholder">
                    <i class="fas fa-home"></i>
                </div>
            </div>
        </div>
    </section>

    <section class="services">
        <div class="container">
            <h2 class="section-title">Dunnellon Standard Cleaning Service</h2>
            <div style="max-width: 800px; margin: 0 auto 3rem;">
                <p style="font-size: 1.1rem; color: #666; text-align: center; line-height: 1.6;">Our standard house cleaning service is perfectly tailored for Dunnellon's unique mix of historic homes, modern riverfront properties, and rural estates. We understand the special care required for homes along the Rainbow River and the charming character of this historic community.</p>
            </div>
            
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2rem; margin-bottom: 3rem;">
                <div style="background: #f8fafa; padding: 2rem; border-radius: 15px;">
                    <h3 style="color: #2f5047; margin-bottom: 1rem;"><i class="fas fa-calendar" style="color: #ff8057; margin-right: 0.5rem;"></i>Flexible Scheduling</h3>
                    <ul style="color: #666; line-height: 1.8;">
                        <li><strong>Weekly:</strong> Perfect for busy Dunnellon families</li>
                        <li><strong>Bi-weekly:</strong> Most popular option for regular maintenance</li>
                        <li><strong>Monthly:</strong> Ideal for well-maintained homes</li>
                        <li><strong>Custom:</strong> We adapt to your schedule and travel</li>
                    </ul>
                </div>
                
                <div style="background: #f8fafa; padding: 2rem; border-radius: 15px;">
                    <h3 style="color: #2f5047; margin-bottom: 1rem;"><i class="fas fa-shield-alt" style="color: #ff8057; margin-right: 0.5rem;"></i>Dunnellon Specialties</h3>
                    <ul style="color: #666; line-height: 1.8;">
                        <li>Historic home experience</li>
                        <li>River humidity management</li>
                        <li>Outdoor lifestyle cleaning</li>
                        <li>Small-town personalized service</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

    <section class="about" style="background: #f8fafa;">
        <div class="container">
            <h2 class="section-title">What's Included in Dunnellon Standard Cleaning</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2rem;">
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-utensils" style="margin-right: 0.5rem;"></i>Kitchen</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Clean and sanitize all surfaces</li>
                        <li>Exterior appliance cleaning</li>
                        <li>Sink and faucet polishing</li>
                        <li>Cabinet exterior cleaning</li>
                        <li>Countertop care (all materials)</li>
                        <li>Trash removal and liner replacement</li>
                    </ul>
                </div>
                
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-bath" style="margin-right: 0.5rem;"></i>Bathrooms</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Complete sanitization</li>
                        <li>Shower and tub deep cleaning</li>
                        <li>Toilet cleaning inside and out</li>
                        <li>Mirror and fixture polishing</li>
                        <li>Tile and grout care</li>
                        <li>Floor mopping and sanitizing</li>
                    </ul>
                </div>
                
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-bed" style="margin-right: 0.5rem;"></i>Living Areas & Bedrooms</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Dusting all surfaces and furniture</li>
                        <li>Vacuuming carpets and rugs</li>
                        <li>Hard floor cleaning and polishing</li>
                        <li>Light organizing and tidying</li>
                        <li>Window sill and baseboard cleaning</li>
                        <li>Trash emptying throughout</li>
                    </ul>
                </div>
                
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-star" style="margin-right: 0.5rem;"></i>Dunnellon Specialties</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>River humidity protection</li>
                        <li>Historic home care</li>
                        <li>Outdoor gear cleaning areas</li>
                        <li>Guest-ready standards</li>
                        <li>Community pride maintenance</li>
                        <li>Seasonal adjustments</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

    <section class="services">
        <div class="container">
            <h2 class="section-title">Why Dunnellon Residents Choose Our Standard Cleaning</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); gap: 2rem; margin-bottom: 3rem;">
                <div style="text-align: center; padding: 2rem;">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem;">
                        <i class="fas fa-home"></i>
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Historic Home Experience</h4>
                    <p style="color: #666; line-height: 1.6;">Specialized care for Dunnellon's historic homes, understanding unique features and preservation needs while maintaining modern cleanliness standards.</p>
                </div>
                
                <div style="text-align: center; padding: 2rem;">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem;">
                        <i class="fas fa-water"></i>
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">River Community Knowledge</h4>
                    <p style="color: #666; line-height: 1.6;">Understanding of humidity challenges from the Rainbow River location and specialized cleaning approaches for riverside living.</p>
                </div>
                
                <div style="text-align: center; padding: 2rem;">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem;">
                        <i class="fas fa-clock"></i>
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Reliable Consistency</h4>
                    <p style="color: #666; line-height: 1.6;">Same team, same high standards, every visit. Dependable service that Dunnellon residents trust for their homes.</p>
                </div>
                
                <div style="text-align: center; padding: 2rem;">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem;">
                        <i class="fas fa-leaf"></i>
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Eco-Friendly Options</h4>
                    <p style="color: #666; line-height: 1.6;">Environmentally friendly products available that protect the natural beauty of the Rainbow River area and local wildlife.</p>
                </div>
            </div>
        </div>
    </section>

    <section class="contact" style="background: #f8fafa;">
        <div class="container">
            <h2 class="section-title">Ready for Regular Dunnellon House Cleaning?</h2>
            <div style="text-align: center; max-width: 600px; margin: 0 auto;">
                <p style="font-size: 1.1rem; color: #666; margin-bottom: 2rem; line-height: 1.6;">Join your Dunnellon neighbors who trust Palma Cleaning for consistent, professional house cleaning. Contact us for your free consultation and quote!</p>
                <div style="display: flex; gap: 1rem; justify-content: center; flex-wrap: wrap; margin-bottom: 2rem;">
                    <a href="/contact" class="btn btn-primary">Book Now</a>
                    <a href="tel:+13527644747" class="btn btn-secondary">Call (352) 764-4747</a>
                </div>
                <div style="background: white; padding: 1.5rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <p style="color: #2f5047; font-weight: 500; margin: 0;">
                        <i class="fas fa-check-circle" style="color: #ff8057; margin-right: 0.5rem;"></i>
                        Licensed & Insured • Dunnellon Experienced • Satisfaction Guaranteed
                    </p>
                </div>
            </div>
        </div>
    </section>

    <section class="services">
        <div class="container">
            <h2 class="section-title">Other Dunnellon Cleaning Services</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); gap: 1.5rem;">
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-magic" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Deep Cleaning</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Comprehensive top-to-bottom cleaning</p>
                    <a href="/dunnellon-deep-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-building" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Commercial Cleaning</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Professional office and business cleaning</p>
                    <a href="/dunnellon-commercial-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-box-open" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Move-in/Move-out</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Complete transition cleaning</p>
                    <a href="/dunnellon-move-in-out-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-hard-hat" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Post-Construction</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Cleanup after renovation projects</p>
                    <a href="/dunnellon-post-construction-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-horse" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Equestrian Properties</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Specialized horse property cleaning</p>
                    <a href="/dunnellon-equestrian-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
            </div>
            
            <div style="text-align: center; margin-top: 2rem;">
                <a href="/dunnellon" class="btn btn-primary">View All Dunnellon Services</a>
            </div>
        </div>
    </section>

    <Footer />

    <script is:inline src="/script-clean.js"></script>
    <script 
        src="https://beta.leadconnectorhq.com/loader.js"  
        data-resources-url="https://beta.leadconnectorhq.com/chat-widget/loader.js" 
        data-widget-id="66fd9ee1d53ad5f3145ea591">
    </script>
</body>
</html>