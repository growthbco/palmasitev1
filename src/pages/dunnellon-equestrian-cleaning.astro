---
// Dunnellon Equestrian Cleaning service page
import Navigation from '../components/Navigation.astro';
import Footer from '../components/Footer.astro';
const title = "Dunnellon Equestrian Property Cleaning | Palma Cleaning - Marion County FL";
const description = "Professional equestrian property cleaning for Dunnellon horse properties in Marion County, FL. Specialized cleaning for horse farms, barns, and equestrian facilities in Dunnellon area.";
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
    <meta name="description" content={description}>
    <meta name="keywords" content="Dunnellon equestrian cleaning, Dunnellon horse property cleaning, Dunnellon barn cleaning, Dunnellon stable cleaning Marion County FL">
    <meta name="robots" content="index, follow">
    
    <link rel="canonical" href="https://palmacleaningco.com/dunnellon-equestrian-cleaning">
    
    <!-- Favicon -->
    <link rel="icon" type="image/svg+xml" href="/favicon.svg">
    <link rel="stylesheet" href="/styles-clean.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Service",
        "name": "Dunnellon Equestrian Property Cleaning",
        "description": "Professional cleaning services for Dunnellon equestrian properties and horse facilities",
        "provider": {
            "@type": "LocalBusiness",
            "name": "Palma Cleaning",
            "telephone": "(352) 764-4747",
            "url": "https://palmacleaningco.com"
        },
        "areaServed": {
            "@type": "Place",
            "name": "Dunnellon, Marion County, FL"
        },
        "offers": {
            "@type": "Offer",
            "description": "Horse property cleaning, barn cleaning, stable maintenance, and equestrian facility care"
        }
    }
    </script>
</head>
<body>
    <Navigation />

    <section class="hero">
        <div class="hero-container">
            <div class="hero-content">
                <h1 class="hero-title">Dunnellon Equestrian Property Cleaning</h1>
                <p class="hero-subtitle">Specialized Cleaning for Dunnellon Horse Properties</p>
                <p class="hero-description">Professional equestrian property cleaning services designed for Dunnellon's rural horse farms, barns, and equestrian facilities. We understand the unique needs of horse properties in this scenic river community and maintain the standards expected by equestrian families.</p>
                <div class="hero-buttons">
                    <a href="/contact" class="btn btn-primary">Book Now</a>
                    <a href="tel:+13527644747" class="btn btn-secondary">Call (352) 764-4747</a>
                </div>
            </div>
            <div class="hero-image">
                <div class="hero-placeholder">
                    <i class="fas fa-horse"></i>
                </div>
            </div>
        </div>
    </section>

    <section class="services">
        <div class="container">
            <h2 class="section-title">Dunnellon Equestrian Cleaning Services</h2>
            <div style="max-width: 800px; margin: 0 auto 3rem;">
                <p style="font-size: 1.1rem; color: #666; text-align: center; line-height: 1.6;">Dunnellon's rural setting along the Rainbow River makes it an ideal location for equestrian properties. Our comprehensive cleaning services maintain both the pristine home environments and specialized horse facility requirements that equestrian families in this area demand.</p>
            </div>
            
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2rem; margin-bottom: 3rem;">
                <div style="background: #f8fafa; padding: 2rem; border-radius: 15px;">
                    <h3 style="color: #2f5047; margin-bottom: 1rem;"><i class="fas fa-home" style="color: #ff8057; margin-right: 0.5rem;"></i>Rural Estate Homes</h3>
                    <ul style="color: #666; line-height: 1.8;">
                        <li><strong>Main Residence:</strong> Full luxury home cleaning</li>
                        <li><strong>Guest Houses:</strong> Visitor accommodation care</li>
                        <li><strong>Staff Quarters:</strong> Employee housing maintenance</li>
                        <li><strong>Entertainment Areas:</strong> Outdoor and indoor spaces</li>
                    </ul>
                </div>
                
                <div style="background: #f8fafa; padding: 2rem; border-radius: 15px;">
                    <h3 style="color: #2f5047; margin-bottom: 1rem;"><i class="fas fa-warehouse" style="color: #ff8057; margin-right: 0.5rem;"></i>Equestrian Facilities</h3>
                    <ul style="color: #666; line-height: 1.8;">
                        <li>Barn and stable cleaning</li>
                        <li>Tack room organization</li>
                        <li>Feed room maintenance</li>
                        <li>Indoor arena cleaning</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

    <section class="about" style="background: #f8fafa;">
        <div class="container">
            <h2 class="section-title">Complete Dunnellon Equestrian Property Care</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2rem;">
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-home" style="margin-right: 0.5rem;"></i>Residence Cleaning</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Rural estate interior cleaning</li>
                        <li>Mud room and boot room care</li>
                        <li>Hardwood and tile floor care</li>
                        <li>Kitchen and bathroom deep cleaning</li>
                        <li>Laundry and utility room cleaning</li>
                        <li>Window and glass cleaning</li>
                        <li>Outdoor living space maintenance</li>
                    </ul>
                </div>
                
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-warehouse" style="margin-right: 0.5rem;"></i>Barn & Stable Care</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Barn aisle and walkway cleaning</li>
                        <li>Stall cleaning and maintenance</li>
                        <li>Feed and water area sanitization</li>
                        <li>Hay storage area cleaning</li>
                        <li>Grooming area maintenance</li>
                        <li>Barn office cleaning</li>
                        <li>Equipment storage organization</li>
                    </ul>
                </div>
                
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-medal" style="margin-right: 0.5rem;"></i>Specialized Areas</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Tack room organization and cleaning</li>
                        <li>Trophy and award display care</li>
                        <li>Saddle and equipment maintenance</li>
                        <li>Show preparation areas</li>
                        <li>Rider lounge and comfort areas</li>
                        <li>Training facility cleaning</li>
                        <li>Guest and visitor areas</li>
                    </ul>
                </div>
                
                <div style="background: white; padding: 2rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #ff8057; margin-bottom: 1rem;"><i class="fas fa-star" style="margin-right: 0.5rem;"></i>Dunnellon Standards</h4>
                    <ul style="color: #666; line-height: 1.6;">
                        <li>Rural property experience</li>
                        <li>Horse-safe cleaning products</li>
                        <li>Flexible scheduling around feeding</li>
                        <li>River environment considerations</li>
                        <li>Large acreage property care</li>
                        <li>Emergency cleaning services</li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

    <section class="contact" style="background: #f8fafa;">
        <div class="container">
            <h2 class="section-title">Ready for Dunnellon Equestrian Property Cleaning?</h2>
            <div style="text-align: center; max-width: 600px; margin: 0 auto;">
                <p style="font-size: 1.1rem; color: #666; margin-bottom: 2rem; line-height: 1.6;">Maintain your Dunnellon equestrian property to the highest standards with our specialized cleaning services. Contact us for a customized cleaning plan for your horse property!</p>
                <div style="display: flex; gap: 1rem; justify-content: center; flex-wrap: wrap; margin-bottom: 2rem;">
                    <a href="/contact" class="btn btn-primary">Book Now</a>
                    <a href="tel:+13527644747" class="btn btn-secondary">Call (352) 764-4747</a>
                </div>
                <div style="background: white; padding: 1.5rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <p style="color: #2f5047; font-weight: 500; margin: 0;">
                        <i class="fas fa-check-circle" style="color: #ff8057; margin-right: 0.5rem;"></i>
                        Licensed & Insured • Equestrian Experienced • Horse-Safe Products
                    </p>
                </div>
            </div>
        </div>
    </section>

    <section class="services">
        <div class="container">
            <h2 class="section-title">Other Dunnellon Cleaning Services</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); gap: 1.5rem;">
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-home" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Standard Cleaning</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Regular weekly, bi-weekly, monthly</p>
                    <a href="/dunnellon-standard-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-magic" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Deep Cleaning</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Comprehensive top-to-bottom cleaning</p>
                    <a href="/dunnellon-deep-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-building" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Commercial Cleaning</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Professional office and business cleaning</p>
                    <a href="/dunnellon-commercial-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-box-open" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Move-in/Move-out</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Complete transition cleaning</p>
                    <a href="/dunnellon-move-in-out-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 10px; text-align: center;">
                    <i class="fas fa-hard-hat" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Post-Construction</h4>
                    <p style="color: #666; font-size: 0.9rem; margin-bottom: 1rem;">Cleanup after renovation projects</p>
                    <a href="/dunnellon-post-construction-cleaning" class="btn btn-secondary" style="padding: 6px 12px; font-size: 0.8rem;">Learn More</a>
                </div>
            </div>
            
            <div style="text-align: center; margin-top: 2rem;">
                <a href="/dunnellon" class="btn btn-primary">View All Dunnellon Services</a>
            </div>
        </div>
    </section>

    <Footer />

    <script is:inline src="/script-clean.js"></script>
    <script 
        src="https://beta.leadconnectorhq.com/loader.js"  
        data-resources-url="https://beta.leadconnectorhq.com/chat-widget/loader.js" 
        data-widget-id="66fd9ee1d53ad5f3145ea591">
    </script>
</body>
</html>