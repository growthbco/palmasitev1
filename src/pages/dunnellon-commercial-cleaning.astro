---
// Dunnellon Commercial Cleaning service page
import Navigation from '../components/Navigation.astro';
import Footer from '../components/Footer.astro';
const title = "Dunnellon Commercial Cleaning | Palma Cleaning - Marion County FL";
const description = "Professional commercial cleaning services for Dunnellon businesses in Marion County, FL. Office cleaning, restaurant cleaning, and commercial facility services for Dunnellon. Licensed & insured.";
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
    <meta name="description" content={description}>
    <meta name="keywords" content="Dunnellon commercial cleaning, Dunnellon office cleaning, Dunnellon business cleaning, Dunnellon restaurant cleaning Marion County FL">
    <meta name="robots" content="index, follow">
    
    <link rel="canonical" href="https://palmacleaningco.com/dunnellon-commercial-cleaning">
    
    <!-- Favicon -->
    <link rel="icon" type="image/svg+xml" href="/favicon.svg">
    <link rel="stylesheet" href="/styles-clean.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Service",
        "name": "Dunnellon Commercial Cleaning",
        "description": "Professional commercial cleaning services for Dunnellon businesses and facilities",
        "provider": {
            "@type": "LocalBusiness",
            "name": "Palma Cleaning",
            "telephone": "(352) 764-4747",
            "url": "https://palmacleaningco.com"
        },
        "areaServed": {
            "@type": "Place",
            "name": "Dunnellon, Marion County, FL"
        },
        "offers": {
            "@type": "Offer",
            "description": "Office cleaning, restaurant cleaning, retail cleaning, and commercial facility maintenance"
        }
    }
    </script>
</head>
<body>
    <Navigation />

    <section class="hero">
        <div class="hero-container">
            <div class="hero-content">
                <h1 class="hero-title">Dunnellon Commercial Cleaning</h1>
                <p class="hero-subtitle">Professional Cleaning for Dunnellon Businesses</p>
                <p class="hero-description">Comprehensive commercial cleaning services designed for Dunnellon's historic downtown businesses, tourism facilities, restaurants, and professional offices. We maintain the cleanliness standards that support this charming river community's economy.</p>
                <div class="hero-buttons">
                    <a href="/contact" class="btn btn-primary">Book Now</a>
                    <a href="tel:+13527644747" class="btn btn-secondary">Call (352) 764-4747</a>
                </div>
            </div>
            <div class="hero-image">
                <div class="hero-placeholder">
                    <i class="fas fa-building"></i>
                </div>
            </div>
        </div>
    </section>

    <section class="services">
        <div class="container">
            <h2 class="section-title">Dunnellon Commercial Cleaning Services</h2>
            <div style="max-width: 800px; margin: 0 auto 3rem;">
                <p style="font-size: 1.1rem; color: #666; text-align: center; line-height: 1.6;">Dunnellon's unique mix of historic charm, tourism appeal, and local businesses requires specialized commercial cleaning that maintains both cleanliness and character. Our professional services support the thriving business community along the Rainbow River.</p>
            </div>
            
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2rem; margin-bottom: 3rem;">
                <div style="background: #f8fafa; padding: 2rem; border-radius: 15px;">
                    <h3 style="color: #2f5047; margin-bottom: 1rem;"><i class="fas fa-store" style="color: #ff8057; margin-right: 0.5rem;"></i>Historic Downtown</h3>
                    <ul style="color: #666; line-height: 1.8;">
                        <li><strong>Retail Shops:</strong> Antique stores, boutiques, gift shops</li>
                        <li><strong>Restaurants & Cafes:</strong> Dining establishments</li>
                        <li><strong>Tourist Facilities:</strong> Visitor centers, museums</li>
                        <li><strong>Historic Buildings:</strong> Heritage-sensitive cleaning</li>
                    </ul>
                </div>
                
                <div style="background: #f8fafa; padding: 2rem; border-radius: 15px;">
                    <h3 style="color: #2f5047; margin-bottom: 1rem;"><i class="fas fa-briefcase" style="color: #ff8057; margin-right: 0.5rem;"></i>Professional Services</h3>
                    <ul style="color: #666; line-height: 1.8;">
                        <li>Law offices and legal services</li>
                        <li>Medical and dental practices</li>
                        <li>Real estate offices</li>
