  { task: "Dust window sills", standard: true, deep: true, moveInOut: true },
  { task: "Dust baseboards", standard: true, deep: true, moveInOut: true },
  { task: "Empty trash", standard: true, deep: true, moveInOut: true },
  { task: "Clean mirrors", standard: true, deep: true, moveInOut: true },
  { task: "Hand wash baseboards", standard: false, deep: true, moveInOut: true },
