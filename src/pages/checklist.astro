  { task: "Wipe cabinet fronts", standard: true, deep: true, moveInOut: true },
  { task: "Scrub and disinfect sinks", standard: true, deep: true, moveInOut: true },
  { task: "Light dish load", standard: true, deep: true, moveInOut: true },
  { task: "Vacuum and/or wash floor", standard: true, deep: true, moveInOut: true },
  { task: "Hand wash baseboards", standard: false, deep: true, moveInOut: true },
  { task: "Degrease and clean oven interior", standard: false, deep: true, moveInOut: true },
