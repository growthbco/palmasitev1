  { task: "Hand wash baseboards", standard: false, deep: true, moveInOut: true },
  { task: "Light sweep of empty garage (if applicable)", standard: false, deep: false, moveInOut: true },
  { task: "Sweep enclosed sunrooms/balconies", standard: false, deep: false, moveInOut: true },
  { task: "Clean behind/under movable appliances", standard: false, deep: false, moveInOut: true },
  { task: "Walls are not included in any service (only cobweb removal)", standard: false, deep: false, moveInOut: false }
