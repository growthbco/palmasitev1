    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
    <meta name="description" content={description}>
    <meta name="keywords" content="The Villages equestrian cleaning, The Villages horse property cleaning, The Villages barn cleaning, The Villages stable cleaning Florida">
    <meta name="robots" content="index, follow">
    
    <link rel="canonical" href="https://palmacleaningco.com/the-villages-equestrian-cleaning">
    
    <!-- Favicon -->
    <link rel="icon" type="image/svg+xml" href="/favicon.svg">
    <link rel="stylesheet" href="/styles-clean.css?v=3">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Service",
        "name": "The Villages Equestrian Property Cleaning",
        "description": "Professional cleaning services for The Villages equestrian properties and horse facilities",
        "provider": {
            "@type": "LocalBusiness",
            "name": "Palma Cleaning",
            "telephone": "(352) 764-4747",
