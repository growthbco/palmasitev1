---
// Quote page with Convertlabs booking embed
import Navigation from '../components/Navigation.astro';
import Footer from '../components/Footer.astro';
const title = "Get Your Free Cleaning Quote | Palma Cleaning - Ocala FL";
const description = "Get your free cleaning quote online! Easy quote form for house cleaning, commercial cleaning, and specialized services in Ocala, Marion County, and The Villages.";
---

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{title}</title>
    <meta name="description" content={description}>
    <meta name="keywords" content="free cleaning quote Ocala FL, cleaning estimate Marion County, house cleaning quote The Villages, online cleaning quote">
    <meta name="robots" content="index, follow">
    
    <link rel="canonical" href="https://palmacleaningco.com/quote">
    <link rel="stylesheet" href="/styles-clean.css">
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@300;400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <style>.Convertlabs{width: 1px;min-width: 100%; height: 1px; min-height: 100%;}</style>
    <script src="https://convertlabs.io/js/booking_embed.js"></script>
    
    <script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "LocalBusiness",
        "name": "Palma Cleaning",
        "image": "https://palmacleaningco.com/images/logo.jpg",
        "description": "Get your free cleaning quote online for professional cleaning services in Ocala, FL",
        "address": {
            "@type": "PostalAddress",
            "addressLocality": "Ocala",
            "addressRegion": "FL",
            "postalCode": "34471",
            "addressCountry": "US"
        },
        "telephone": "(352) 764-4747",
        "email": "hello@palmacleaningco.com",
        "url": "https://palmacleaningco.com/quote",
        "geo": {
            "@type": "GeoCoordinates",
            "latitude": "29.1872",
            "longitude": "-82.1401"
        },
        "openingHours": "Mo-Fr 08:00-18:00",
        "priceRange": "$$"
    }
    </script>
</head>
<body>
    <Navigation />

    <section class="hero" style="padding: 6rem 0 4rem;">
        <div class="hero-container">
            <div class="hero-content" style="text-align: center; grid-column: 1 / -1;">
                <h1 class="hero-title">Get Your Free Cleaning Quote</h1>
                <p class="hero-subtitle">Professional Cleaning Services in Ocala, Marion County & The Villages</p>
                <p class="hero-description" style="max-width: 800px; margin: 0 auto;">Complete the form below to receive your personalized cleaning quote. We provide detailed estimates for all our services including house cleaning, commercial cleaning, and specialized services.</p>
                
                <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(200px, 1fr)); gap: 1rem; margin: 2rem 0; max-width: 600px; margin-left: auto; margin-right: auto;">
                    <div style="background: rgba(255, 255, 255, 0.15); padding: 1rem; border-radius: 10px;">
                        <i class="fas fa-clock" style="font-size: 1.5rem; margin-bottom: 0.5rem;"></i>
                        <p style="margin: 0; font-weight: 500;">24-Hour Response</p>
                    </div>
                    <div style="background: rgba(255, 255, 255, 0.15); padding: 1rem; border-radius: 10px;">
                        <i class="fas fa-dollar-sign" style="font-size: 1.5rem; margin-bottom: 0.5rem;"></i>
                        <p style="margin: 0; font-weight: 500;">No Obligation</p>
                    </div>
                    <div style="background: rgba(255, 255, 255, 0.15); padding: 1rem; border-radius: 10px;">
                        <i class="fas fa-shield-alt" style="font-size: 1.5rem; margin-bottom: 0.5rem;"></i>
                        <p style="margin: 0; font-weight: 500;">Licensed & Insured</p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="services" style="background: white; padding: 3rem 0;">
        <div class="container">
            <div style="max-width: 1000px; margin: 0 auto;">
                <div style="background: #f8fafa; padding: 2rem; border-radius: 20px; box-shadow: 0 10px 30px rgba(0,0,0,0.1); margin-bottom: 2rem;">
                    <h2 style="text-align: center; color: #2f5047; margin-bottom: 1.5rem;">Complete Your Quote Request</h2>
                    <p style="text-align: center; color: #666; margin-bottom: 2rem; line-height: 1.6;">Please fill out all fields accurately to receive the most precise quote for your cleaning needs.</p>
                    
                    <iframe src="https://convertlabs.io/quote_form/3473" frameborder="0" scrolling="no" style="width: 100%; min-height: 800px; border-radius: 15px;" class="Convertlabs"></iframe>
                </div>
            </div>
        </div>
    </section>

    <section class="about" style="background: #f8fafa; padding: 3rem 0;">
        <div class="container">
            <h2 class="section-title">What Happens Next?</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2rem; max-width: 900px; margin: 0 auto;">
                <div style="text-align: center; padding: 2rem; background: white; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem; font-weight: bold;">
                        1
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Quote Review</h4>
                    <p style="color: #666; line-height: 1.6;">We'll review your requirements and prepare a detailed, personalized quote for your cleaning needs.</p>
                </div>
                
                <div style="text-align: center; padding: 2rem; background: white; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem; font-weight: bold;">
                        2
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Quick Response</h4>
                    <p style="color: #666; line-height: 1.6;">You'll receive your detailed quote within 24 hours via email or phone, whichever you prefer.</p>
                </div>
                
                <div style="text-align: center; padding: 2rem; background: white; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <div style="background: #ff8057; color: white; width: 60px; height: 60px; border-radius: 50%; display: flex; align-items: center; justify-content: center; margin: 0 auto 1rem; font-size: 1.5rem; font-weight: bold;">
                        3
                    </div>
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Schedule Service</h4>
                    <p style="color: #666; line-height: 1.6;">Once you approve the quote, we'll schedule your cleaning service at a time that's convenient for you.</p>
                </div>
            </div>
        </div>
    </section>

    <section class="services" style="padding: 3rem 0;">
        <div class="container">
            <h2 class="section-title">Our Cleaning Services</h2>
            <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(250px, 1fr)); gap: 1.5rem; max-width: 1000px; margin: 0 auto;">
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 15px; text-align: center;">
                    <i class="fas fa-home" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Standard House Cleaning</h4>
                    <p style="color: #666; font-size: 0.9rem;">Regular weekly, bi-weekly, or monthly cleaning</p>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 15px; text-align: center;">
                    <i class="fas fa-magic" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Deep Cleaning</h4>
                    <p style="color: #666; font-size: 0.9rem;">Comprehensive top-to-bottom cleaning</p>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 15px; text-align: center;">
                    <i class="fas fa-building" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Commercial Cleaning</h4>
                    <p style="color: #666; font-size: 0.9rem;">Professional office and business cleaning</p>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 15px; text-align: center;">
                    <i class="fas fa-box-open" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Move-in/Move-out</h4>
                    <p style="color: #666; font-size: 0.9rem;">Complete cleaning for property transitions</p>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 15px; text-align: center;">
                    <i class="fas fa-hard-hat" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Post-Construction</h4>
                    <p style="color: #666; font-size: 0.9rem;">Cleanup after construction and renovation</p>
                </div>
                
                <div style="background: #f8fafa; padding: 1.5rem; border-radius: 15px; text-align: center;">
                    <i class="fas fa-horse" style="font-size: 2rem; color: #ff8057; margin-bottom: 1rem;"></i>
                    <h4 style="color: #2f5047; margin-bottom: 0.5rem;">Equestrian Properties</h4>
                    <p style="color: #666; font-size: 0.9rem;">Specialized cleaning for horse properties</p>
                </div>
            </div>
        </div>
    </section>

    <section class="contact" style="background: #f8fafa; padding: 3rem 0;">
        <div class="container">
            <div style="text-align: center; max-width: 600px; margin: 0 auto;">
                <h2 class="section-title">Need Help or Have Questions?</h2>
                <p style="font-size: 1.1rem; color: #666; margin-bottom: 2rem; line-height: 1.6;">Our team is here to help! Contact us directly if you need assistance with your quote or have specific questions.</p>
                
                <div style="display: grid; grid-template-columns: repeat(auto-fit, minmax(200px, 1fr)); gap: 1rem; margin-bottom: 2rem;">
                    <a href="tel:+13527644747" class="btn btn-primary" style="padding: 1rem; font-size: 1.1rem; text-decoration: none;">
                        <i class="fas fa-phone" style="margin-right: 0.5rem;"></i>
                        Call (352) 764-4747
                    </a>
                    <a href="/contact" class="btn btn-secondary" style="padding: 1rem; font-size: 1.1rem; text-decoration: none;">
                        <i class="fas fa-envelope" style="margin-right: 0.5rem;"></i>
                        Contact Form
                    </a>
                </div>
                
                <div style="background: white; padding: 1.5rem; border-radius: 15px; box-shadow: 0 5px 20px rgba(0,0,0,0.08);">
                    <h4 style="color: #2f5047; margin-bottom: 1rem;">Business Hours</h4>
                    <p style="color: #666; margin: 0; line-height: 1.8;">
                        <strong>Monday - Friday:</strong> 8:00 AM - 6:00 PM<br>
                        <strong>Response Time:</strong> Within 24 hours<br>
                        <strong>Emergency Service:</strong> Available weekends
                    </p>
                </div>
            </div>
        </div>
    </section>

    <Footer />

    <script is:inline src="/script-clean.js"></script>
    <script 
        src="https://beta.leadconnectorhq.com/loader.js"  
        data-resources-url="https://beta.leadconnectorhq.com/chat-widget/loader.js" 
        data-widget-id="66fd9ee1d53ad5f3145ea591">
    </script>
</body>
</html>